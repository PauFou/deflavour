<div class="nested-form-wrapper" data-new-record="<%= true %>">
  <%= f.input :spirit, class:"experience-label" , collection: Spirit.all, input_html: { multiple: true, data: { controller: "tom-select", tom_select_options_value: { tags: true } } } %>

  <button type="button" class="experience-button-remove" data-action="nested-form#remove"><i class="fa-solid fa-trash"></i></button>

  <%= f.hidden_field :_destroy %>
</div>
